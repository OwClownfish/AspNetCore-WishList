@using WishList.Models;
@{
    <h1>Wishlist</h1>
    <a asp-action="Index" asp-controller="Item">View wishlist</a>
    <a asp-action="create">Add item</a>
    <ul>
    @foreach (Item i in Model)
    {
        <li>@i.Description</li>
        <a asp-action="delete" asp-route-id=@i.Id>delete</a>
    }
</ul>
}
